{
    "data": {
        "train_path": "../input/titanic/train.csv",
        "test_path": "../input/titanic/test.csv",
        "output_dir": ".",
        "type": "table",
        "pred_cols": ["Survived"],
        "id_col": "PassengerId"
    },
    "pre": {
        "train_mode": "clf",
        "y_pre": false,
        "y_scaler": "standard",
        "table": {
            "adhoc": {
                "myfunc": "titanic",
                "methods": [
                    "translate_honorific_title",
                    "translate_age",
                    "translate_familystatus"
                ]
            },
            "del": [
                "HonorificTitle",
                "Name",
                "Ticket",
                "Fare",
                "Cabin",
                "Embarked"
            ],
            "category": {
                "target": [
                    "Pclass",
                    "FamilyStatus"
                ],
                "model": "onehot"
            },
            "x_scaler": "standard",
            "dimension": false,
            "adversarial": false
        }
    },
    "fit": {
        "train_mode": "clf",
        "ensemble": {
            "mode": "stacking",
            "model": "log_reg",
            "modelname": "titanic",
            "k": 10,
            "seed": 111
        },
        "scoring": "accuracy",
        "single_models": [
            {
                "model": "gbdt_clf",
                "modelname": "titanic_gbdt",
                "fit_params": {
                },
                "params": {
                    "n_estimators": [1, 2, 3, 4, 5],
                    "max_depth": [1, 2, 3, 4, 5]
                }
            }
        ],
        "post": {
            "myfunc": false,
            "methods": [
            ]
        }
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
