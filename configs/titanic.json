{
    "data": {
        "train_path": "../input/titanic/train.csv",
        "test_path": "../input/titanic/test.csv",
        "pred_col": "Survived",
        "id_col": "PassengerId",
        "random": false,
        "adversarial": {
            "model": "log_reg",
            "scoring": "accuracy",
            "cv": 3,
            "n_jobs": -1,
            "params" : {
                "penalty": ["l1", "l2"],
                "C": [1, 2, 3, 4, 5]
            }
        }
    },
    "translate": {
        "adhoc": {
            "myfunc": "titanic",
            "methods": [
                "translate_age",
                "translate_familystatus"
            ]
        },
        "del": [
            "Name",
            "Ticket",
            "Fare",
            "Cabin",
            "Embarked"
        ],
        "category": [
            "Pclass",
            "FamilyStatus"
        ],
        "dimension": false
    },
    "fit": {
        "pre": false,
        "mode": "clf",
        "model": "gbdt_clf",
        "modelname": "titanic",
        "scoring": "accuracy",
        "cv": 3,
        "n_jobs": -1,
        "fit_params" : {
        },
        "params" : {
            "n_estimators": [1, 2, 3, 4, 5],
            "max_depth": [1, 2, 3, 4, 5]
        }
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
