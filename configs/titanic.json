{
    "data": {
        "train_path": "../input/titanic/train.csv",
        "test_path": "../input/titanic/test.csv",
        "pred_col": "Survived",
        "id_col": "PassengerId",
        "random": false,
        "adversarial": {
            "model": "log_reg",
            "scoring": "accuracy",
            "cv": 3,
            "n_jobs": -1,
            "params" : {
                "penalty": ["l2"],
                "C": [1, 2, 3, 4, 5]
            }
        }
    },
    "translate": {
        "adhoc": {
            "myfunc": "titanic",
            "methods": [
                "translate_honorific_title",
                "translate_age",
                "translate_familystatus"
            ]
        },
        "del": [
            "HonorificTitle",
            "Name",
            "Ticket",
            "Fare",
            "Cabin",
            "Embarked"
        ],
        "category": [
            "Pclass",
            "FamilyStatus"
        ],
        "dimension": false
    },
    "fit": {
        "y_pre": false,
        "train_mode": "clf",
        "ensemble": {
            "mode": "stacking",
            "model": "log_reg",
            "modelname": "titanic",
            "k": 10,
            "seed": 111
        },
        "scoring": "accuracy",
        "single_models": [
            {
                "model": "gbdt_clf",
                "modelname": "titanic_gbdt",
                "cv": 3,
                "n_jobs": -1,
                "fit_params" : {
                },
                "params" : {
                    "n_estimators": [1, 2, 3, 4, 5],
                    "max_depth": [1, 2, 3, 4, 5]
                }
            }
        ],
        "post": {
            "myfunc": false,
            "methods": [
            ]
        }
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
