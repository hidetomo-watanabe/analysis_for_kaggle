{
    "data": {
        "train_path": "../input/digit/train.csv",
        "test_path": "../input/digit/test.csv",
        "pred_cols": ["label"],
        "id_col": "ImageId"
    },
    "translate": {
        "adhoc": {
            "myfunc": "digit",
            "methods": [
                "add_image_id"
            ]
        },
        "del": [
        ],
        "category": [
        ],
        "dimension": false,
        "adversarial": false
    },
    "fit": {
        "y_pre": false,
        "train_mode": "clf",
        "ensemble": {
            "mode": "stacking",
            "model": "log_reg",
            "modelname": "digit",
            "k": 10,
            "seed": 111
        },
        "scoring": "accuracy",
        "single_models": [
            {
                "model": "svc",
                "modelname": "digit_svc",
                "cv": 3,
                "n_jobs": -1,
                "max_evals": 5,
                "fit_params": {
                },
                "params": {
                    "probability": [true],
                    "C": [1, 2, 3, 4, 5],
                    "degree": [1, 2, 3, 4, 5],
                    "kernel": ["rbf", "linear", "poly", "sigmoid"]
                }
            }
        ],
        "post": {
            "myfunc": "digit",
            "methods": [
                "translate_label2upper"
            ]
        }
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
