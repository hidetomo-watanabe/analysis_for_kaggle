{
    "data": {
        "train_path": "../input/gendered_pronoun/train.tsv",
        "test_path": "../input/gendered_pronoun/test_stage_2.tsv",
        "output_dir": ".",
        "type": "table",
        "pred_cols": ["A","B","NEITHER"],
        "id_col": "ID",
        "delimiter": "\t"
    },
    "pre": {
        "train_mode": "clf",
        "y_scaler": "standard",
        "table": {
            "adhoc_df": {
                "myfunc": "gendered_pronoun",
                "methods": [
                    "rename_columns",
                    "get_base_features",
                    "get_nlp_features",
                    "get_train_neither"
                ]
            },
            "deletion": [
                "Pronoun",
                "Text",
                "A-noun",
                "B-noun",
                "URL"
            ],
            "missing_imputation": true,
            "category": {
                "target": [
                ],
                "model": "onehot"
            },
            "x_scaler": "standard",
            "undersampling": "random"
        }
    },
    "fit": {
        "myfunc": "gendred_pronoun",
        "train_mode": "clf",
        "scoring": "neg_log_loss",
