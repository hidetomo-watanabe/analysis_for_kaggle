            "mode": "stacking",
            "model": "log_reg",
            "modelname": "tmp_gp",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "lgb_clf",
                "multiclass": "onevsrest",
                "modelname": "tmp",
                "n_trials": 50,
                "fit_params" : {
                },
                "params" : {
                    "num_leaves": [5, 10, 20, 30, 50, 100],
                    "max_depth": [5, 10, 20, 30, 50, 100],
                    "n_estimators": [5, 10, 20, 30, 50, 100]
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
