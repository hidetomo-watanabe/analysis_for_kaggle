{
    "data": {
        "train_path": "../input/quora/train.csv",
        "test_path": "../input/quora/test.csv",
        "output_dir": ".",
        "type": "table",
        "pred_cols": ["target"],
        "id_col": "qid"
    },
    "pre": {
        "train_mode": "clf",
        "y_pre": false,
        "y_scaler": "standard",
        "table": {
            "adhoc_df": {
                "myfunc": "quora",
                "methods": [
                    "translate_text2seq"
                ]
            },
            "del": [
            ],
            "missing": true,
            "category": {
                "target": [
                ],
                "model": "onehot"
            },
            "x_scaler": "standard",
            "dimension": false,
            "adversarial": false
        }
    },
    "fit": {
        "train_mode": "clf",
        "scoring": "f1",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "modelname": "submission",
            "k": 10,
            "proportion": 0.2
        },
        "single_model_configs": [
            {
                "model": "keras_clf",
                "modelname": "quora_keras",
                "keras_build": "quora",
                "fit_params": {
                },
                "params": {
                }
            }
        ]
    },
    "post": {
        "myfunc": "quora",
        "methods": [
            "translate_target2prediction"
        ]
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
