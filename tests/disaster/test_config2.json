{
    "data": {
        "train_path": "./tests/disaster/train.csv",
        "test_path": "./tests/disaster/test.csv",
        "output_dir": "./outputs",
        "type": "table",
        "pred_cols": ["target"],
        "id_col": "id"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "deletion": [
                "keyword",
                "location"
            ]
        }
    },
    "fit": {
        "train_mode": "clf",
        "scoring": "f1",
        "cv": {
            "fold": "stratifiedk",
            "num": 3
        },
        "modelname": "tmp_disaster2",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10,
            "proportion": 0.2
        },
        "single_model_configs": [
            {
                "model": "bert_clf",
                "train_all": true,
                "fit_params": {
                },
                "params": {
                    "epochs": [50],
                    "learning_rate": [1e-5]
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
