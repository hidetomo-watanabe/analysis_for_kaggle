{
    "data": {
        "train_path": "./tests/cactus_part/train.csv",
        "test_path": "./tests/cactus_part/test.csv",
        "output_dir": "./outputs",
        "type": "image",
        "pred_cols": ["has_cactus"],
        "id_col": "id"
    },
    "pre": {
        "train_mode": "clf",
        "image": {
            "train_img_dir": "./tests/cactus_part/train",
            "test_img_dir": "./tests/cactus_part/test",
            "img_path_col": "id",
            "resize": {
                "x": 32,
                "y": 32
            },
            "augmentation": {
                "conf": {
                    "rotation_range": 20,
                    "channel_shift_range": true,
                    "horizontal_flip": true,
                    "vertical_flip": true
                },
                "ratio": 3
            }
        }
    },
    "fit": {
        "myfunc": "cactus",
        "train_mode": "clf",
        "scoring": "roc_auc",
        "cv": {
            "fold": "stratifiedk",
            "num": 3
        },
        "modelname": "tmp_cactus",
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "keras_clf",
                "fit_params": {
                    "epochs": 3,
                    "batch_size": 50,
                    "early_stopping": {
                        "monitor": "loss",
                        "patience": 1
                    }
                },
                "params": {
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
