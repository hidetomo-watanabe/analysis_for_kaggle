{
    "data": {
        "train_path": "./tests/titanic/train.csv",
        "test_path": "./tests/titanic/test.csv",
        "output_dir": "./outputs",
        "type": "table",
        "pred_cols": ["Survived"],
        "id_col": "PassengerId"
    },
    "pre": {
        "train_mode": "clf",
        "table": {
            "adhoc_df": {
                "myfunc": "titanic",
                "methods": [
                    "translate_honorific_title",
                    "translate_age",
                    "translate_familystatus"
                ]
            },
            "del": [
                "HonorificTitle",
                "Name",
                "Ticket",
                "Fare",
                "Cabin",
                "Embarked"
            ],
            "missing": true,
            "category": {
                "target": [
                    "FamilyStatus"
                ],
                "model": "onehot"
            },
            "x_scaler": "standard",
            "adversarial_validation": {
                "scoring": "accuracy",
                "model_config": {
                    "model": "rf_clf",
                    "params": {
                        "n_estimators": [3, 5, 10],
                        "max_depth": [3, 5, 10]
                    }
                },
                "threshold": 0.0,
                "add_column": false
            }
        }
    },
    "fit": {
        "myfunc": "titanic",
        "train_mode": "clf",
        "scoring": "accuracy",
        "cv": {
            "fold": "stratifiedk",
            "num": 3
        },
        "ensemble_model_config": {
            "mode": "stacking",
            "model": "log_reg",
            "modelname": "tmp_titanic3",
            "k": 10
        },
        "single_model_configs": [
            {
                "model": "torch_clf",
                "modelname": "tmp",
                "fit_params": {
                },
                "params": {
                    "max_epochs": [3],
                    "batch_size": [50]
                }
            }
        ]
    },
    "post": false,
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
