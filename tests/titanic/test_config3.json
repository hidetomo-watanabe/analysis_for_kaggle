{
    "data": {
        "train_path": "./tests/titanic/train.csv",
        "test_path": "./tests/titanic/test.csv",
        "pred_cols": ["Survived"],
        "id_col": "PassengerId"
    },
    "pre": {
        "train_mode": "clf",
        "adhoc": {
            "myfunc": "titanic",
            "methods": [
                "translate_honorific_title",
                "translate_age",
                "translate_familystatus"
            ]
        },
        "del": [
            "HonorificTitle",
            "Name",
            "Ticket",
            "Fare",
            "Cabin",
            "Embarked"
        ],
        "category": {
            "target": [
                "Pclass",
                "FamilyStatus"
            ],
            "model": "onehot"
        },
        "y_pre": false,
        "dimension": false,
        "adversarial": {
            "model": "rf_clf",
            "scoring": "accuracy",
            "cv": 3,
            "n_jobs": -1,
            "params": {
                "n_estimators": [3, 5, 10],
                "max_depth": [3, 5, 10]
            },
            "threshold": 0.0,
            "add_column": false
        }
    },
    "fit": {
        "myfunc": "titanic",
        "train_mode": "clf",
        "ensemble": {
            "mode": "stacking",
            "model": "log_reg",
            "modelname": "tmp_titanic3",
            "k": 10,
            "seed": 111
        },
        "scoring": "accuracy",
        "single_models": [
            {
                "model": "keras_clf",
                "modelname": "tmp",
                "cv": 3,
                "n_jobs": -1,
                "fit_params": {
                },
                "params": {
                }
            }
        ]
    },
    "post": {
        "myfunc": false,
        "methods": [
        ]
    },
    "notify": {
        "slack": "https://hooks.slack.com/services/hogehoge"
    }
}
